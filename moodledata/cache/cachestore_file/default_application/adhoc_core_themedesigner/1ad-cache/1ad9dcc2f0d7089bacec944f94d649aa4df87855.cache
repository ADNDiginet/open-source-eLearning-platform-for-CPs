/* Custom CSS Settings */
/* Add the CSS styles from the CSS custom box */

.subcategories
{
    display: flex;
    justify-content: center;
    margin-top:30px;
}
.cards
{
   height:150px;
   background-color: lightblue;
   margin-right:1rem;
   border: 3px solid darkblue;
}
.course_category_tree .category > .info > .categoryname {
   margin-top: unset; 
   font-size:22px;
   line-height: 9.3rem;
 }
.course_category_tree .category > .info > .categoryname a {
   font-weight:bold;
 }
.courses
{
   display:unset;
}
.with_children .content {
display:none;
}
.android .subcategories {
flex-wrap:wrap;
}
.android .category{
    margin: 1rem;
    width: 100%;
}
.android .courses {
    display: flex;
    flex-wrap: wrap;
}
.ios .subcategories {
flex-wrap:wrap;
}
.ios .category{
    margin: 1rem;
    width: 100%;
}
.ios .courses {
    display: flex;
    flex-wrap: wrap;
}
.bd-highlight > h1{
font-size:1.5rem !important
}
#page-header {
background-color:#06006e
}
#page-header h1 {
padding-left: 16px;
}


